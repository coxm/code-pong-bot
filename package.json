{
  "name": "code-pong-bot",
  "version": "0.0.0",
  "description": "An IRC bot and HTTP server for code pong",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run lint",
    "postdeploy": "npm install && knex migrate:latest",
    "migrate": "knex migrate:latest",
    "start": "node index.js",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc.yaml index.js lib/*.js"
  },
  "keywords": [
    "code",
    "pong",
    "golf",
    "irc",
    "bot",
    "server"
  ],
  "author": "coxm <coxm@users.noreply.github.com>",
  "license": "GPL-3.0+",
  "dependencies": {
    "cli-color": "^1.2.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "irc": "^0.5.2",
    "knex": "^2.4.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "eslint": "^5.6.0"
  }
}
